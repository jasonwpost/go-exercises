<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Hello World!</title>
  </head>
  <body>
    {{ if gt .Score1 .Score2}}
      Score1 > Score2
    {{end}}

    {{ if lt .Score1 .Score2}}
      Score1 < Score2
    {{end}}

    {{ if eq .Score1 .Score2}}
      Score1 = Score2
    {{end}}

    {{ if lt .Score1 10}}
      LAST ONE: SCORE 1 < 10
    {{end}}
  </body>
</html>
